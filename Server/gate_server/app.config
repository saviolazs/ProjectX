<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!--必须配置
        Game.Port:监听端口
        Product.Code:产品代码
        Product.ServerId:产品游服代码
        Redis.Host:Redis主机地址
        Redis.Port:Redis主机端口
        Redis.Password:密码
        Redis.Db:数据库位置
    -->
    <add key="Game.Port" value="9001" />
    <add key="Product.Code" value="1" />
    <add key="Product.ServerId" value="1" />
    <add key="Redis.Host" value="127.0.0.1" />
    <add key="Redis.Db" value="0" />
    <add key="Game.Action.Script.TypeName" value="ProjectX.Script.Action.Action{0}" />
    <add key="Python_Disable"  value="true"/>
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
</configuration>
